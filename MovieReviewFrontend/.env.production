# Production Environment Variables for CinemaVerse Frontend

# API Base URL - Will be replaced by Render.com environment variable
REACT_APP_API_BASE_URL=https://cinemaverse-backend.onrender.com

# Environment
REACT_APP_ENV=production

# Build optimizations
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true

# Performance optimizations
REACT_APP_ENABLE_ANALYTICS=true